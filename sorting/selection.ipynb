   "source": [
    "arr = [7, 5, 9, 0, 3, 1, 6, 2, 4, 8]\n",
    "for i in range(len(arr)) :\n",
    "    min_idx = i\n",
    "    for j in range(i+1, len(arr)) :\n",
    "        if arr[min_idx] > arr[j] :\n",
    "            min_idx = j  \n",
    "    \n",
    "    arr[min_idx], arr[i] = arr[i], arr[min_idx]\n",
    "\n",
    "print(arr)\n"
   ]
  }
